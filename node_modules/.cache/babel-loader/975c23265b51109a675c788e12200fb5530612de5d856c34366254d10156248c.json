{"ast":null,"code":"import HeaderBar from '@/components/HeaderBar.vue';\nimport PopupPost from '@/components/PopupPost.vue';\nimport { ref } from 'vue';\nimport { createPost, getUserPosts } from '@/services/PostService.js';\nexport default {\n  components: {\n    HeaderBar: HeaderBar,\n    PopupPost: PopupPost\n  },\n  data() {\n    return {\n      userPosts: []\n    };\n  },\n  mounted() {\n    getUserPosts().then(res => {\n      if (res.status === 200) {\n        this.userPosts = res.data;\n        console.log(this.mentors);\n      }\n    }).catch(e => {\n      console.log(e);\n    });\n  },\n  setup() {\n    const popupTriggers = ref({\n      buttonTrigger: false\n    });\n    const togglePopup = trigger => {\n      popupTriggers.value[trigger] = !popupTriggers.value[trigger];\n    };\n    return {\n      popupTriggers,\n      togglePopup\n    };\n  },\n  methods: {\n    funcLike(btn) {\n      var id_button = btn;\n      //console.log(\"button\" + btn);\n      var tmp1 = \"like\" + \"_\" + id_button;\n      var tmp2 = \"blue\" + \"_\" + id_button;\n      var x = document.getElementById(tmp1);\n      var y = document.getElementById(tmp2);\n      if (x.style.display === \"none\" && y.style.display === \"block\") {\n        x.style.display = \"block\";\n        y.style.display = \"none\";\n      } else {\n        x.style.display = \"none\";\n        y.style.display = \"block\";\n      }\n    },\n    togglePlayPause() {\n      var audio = document.getElementById(\"audio\");\n      var playpause = document.getElementById(\"play\");\n      if (audio.paused || audio.ended) {\n        playpause.title = \"Pause\";\n        audio.play();\n      } else {\n        playpause.title = \"Play\";\n        audio.pause();\n      }\n    }\n\n    // $(function () {\n    //   var stickyHeaderTop = $(\"#stickytypeheader\").offset().top;\n\n    //   $(window).scroll(function () {\n    //     if ($(window).scrollTop() > stickyHeaderTop) {\n    //       $(\"#stickytypeheader\").css({ position: \"fixed\", top: \"0px\" });\n    //       $(\"#sticky\").css(\"display\", \"block\");\n    //     } else {\n    //       $(\"#stickytypeheader\").css({ position: \"static\", top: \"0px\" });\n    //       $(\"#sticky\").css(\"display\", \"none\");\n    //     }\n    //   });\n    // }),\n  }\n};","map":{"version":3,"names":["HeaderBar","PopupPost","ref","createPost","getUserPosts","components","data","userPosts","mounted","then","res","status","console","log","mentors","catch","e","setup","popupTriggers","buttonTrigger","togglePopup","trigger","value","methods","funcLike","btn","id_button","tmp1","tmp2","x","document","getElementById","y","style","display","togglePlayPause","audio","playpause","paused","ended","title","play","pause"],"sources":["D:\\Capstone Project\\Vue\\mentore\\src\\views\\UserPost.vue"],"sourcesContent":["<template>\r\n\r\n    <div class=\"page-wrapper\">\r\n      <!-- Preloader -->\r\n      <div class=\"preloader\"></div>\r\n      <header-bar></header-bar>\r\n      <!-- End Main Header -->\r\n      <!-- Slider Section Two -->\r\n      <section class=\"slider-section-two\">\r\n        <div class=\"container\">\r\n          <div class=\"row justify-content-md-center\" id=\"content\">\r\n            <div class=\"col-md-8 feeds\">\r\n            <!-- User Create Post -->   \r\n            <div class=\"feed d-flex flex-column\"> \r\n                <div class=\"p-2\">\r\n                    <button @click=\"() => togglePopup('buttonTrigger')\">\r\n                        <img\r\n                        :src=\"require('../assets/images/add.jpg')\" style=\"width:2%\"/>\r\n                        Tạo bài đăng của bạn\r\n                    </button>\r\n                    <popup-post v-if=\"popupTriggers.buttonTrigger\" \r\n                        :togglePopup=\"() => togglePopup('buttonTrigger')\"></popup-post>\r\n                </div>\r\n            </div>\r\n\r\n              <div v-for=\"post in userPosts\" :key=\"post.id\">\r\n                <div class=\"feed d-flex flex-column feed1\">\r\n                  <div class=\"p-2\" style=\"height: 130px\">\r\n                    <div style=\"position: relative\">\r\n                        <img\r\n                          src=\"@/assets/images/user/user1.png\"\r\n                          alt=\"Avatar\"\r\n                          class=\"avatar\"\r\n                          style=\"cursor: pointer\"\r\n                        />\r\n                        <p\r\n                          class=\"name-user\"\r\n                          style=\"\r\n                            right: -95px;\r\n                            bottom: 75px;\r\n                            font-weight: bold;\r\n                            font-size: 16px;\r\n                          \"\r\n                        >\r\n                          Tuong Vy L\r\n                        </p>\r\n                      <p\r\n                        class=\"name-user\"\r\n                        style=\"\r\n                          right: -95px;\r\n                          bottom: 85px;\r\n                          font-weight: bold;\r\n                          font-size: 13px;\r\n                        \"\r\n                      >\r\n                        15 phút trước\r\n                      </p>\r\n                    </div>\r\n    \r\n                  <div class=\"p-2\">\r\n                    <div class=\"text-feed\">\r\n                      <p style=\"font-weight: bold; color: black; bottom: 15px\">\r\n                        {{ post.title }}\r\n                      </p>\r\n\r\n                      <p>\r\n                      {{ post.content }}\r\n                      <br />\r\n                      </p>\r\n                    </div>\r\n                    <div style=\"height: 425px; width: 100%;\">\r\n                      <img\r\n                      :src=\"post.fileUrl\" cursor: pointer/>\r\n                    </div>\r\n                  \r\n                  </div